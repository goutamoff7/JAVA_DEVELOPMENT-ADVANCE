<!--Search for spring 6 bean configuration xml and copied it.-->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    
<!--    Inner Bean, The Laptop object here can't be used by Programmer Class-->
    <bean class="org.example.Developer">
        <property name="age" value="24"/>
        <property name="laptop">
            <bean class="org.example.Laptop">
            </bean>
        </property>
    </bean>

    <bean class="org.example.Programmer">
        <property name="laptop" ref="lap"/>
    </bean>

    <bean id="lap" class="org.example.Laptop" primary="true">
    </bean>
    
</beans>

